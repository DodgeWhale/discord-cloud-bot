{
	"name": "discord-bot",
	"description": "Discord bot written in TypeScript for Google App Engine.",
	"version": "0.0.1",
	"private": true,
	"license": "Apache Version 2.0",
	"author": "DodgeWhale",
	"engines": {
		"node": "14.x.x"
	},
	"scripts": {
		"prepare": "npm run gcp-build",
		"pretest": "npm run gcp-build",
		"test": "mocha test/*.test.js --exit",
		"posttest": "npm run lint",
		"lint": "tslint -p .",
		"start": "node ./deploy/app.js",
		"gcp-build": "tsc -p .",
		"deploy": "gcloud app deploy"
	},
	"dependencies": {
		"@discordjs/opus": "^0.5.0",
		"@types/express": "^4.17.11",
		"@types/node": "^14.14.7",
		"body-parser": "^1.19.0",
		"discord.js": "^12.5.3",
		"dotenv": "^9.0.2",
		"express": "^4.16.3",
		"ffmpeg-static": "^4.3.0",
		"typescript": "^4.0.0"
	},
	"devDependencies": {
		"chai": "^4.2.0",
		"mocha": "^8.0.0",
		"tslint": "^6.0.0",
		"typescript": "^4.0.0",
		"wait-port": "^0.2.7"
	}
}
